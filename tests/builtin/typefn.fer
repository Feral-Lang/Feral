# type function test for one of the internal types - to ensure *Ty globals are working correctly
let assert = import('std/assert');

let i = 5;

assert.eq(i, 5);

let func in IntTy = fn() {
    return ref(self);
};

let p = i.func();
p = 10;

assert.eq(p, 10);
assert.eq(i, 10);

let copyCtr = 0;

let _copy_ in IntTy = fn() {
    ++copyCtr;
    let res = 0;
    res = self;
    return res;
};

let b = p;
assert.eq(copyCtr, 1);