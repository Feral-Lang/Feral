# logical
assert('hello' < 'ifmmp');
assert('hello' > 'gdkkn');
assert('hello' <= 'hello');
assert('hello' >= 'hello');
assert('hello' == 'hello');
assert('hello' != 'world');

# arithmetic
assert('hello ' + 'world' == 'hello world');
assert('ab' * 3 == 'ababab');

let a = 'ab', b = 'cd';

assert((a += b) == 'abcd');
assert((a *= 2) == 'abcdabcd');

assert(a[0] == 'a');
assert(a[10] == nil);
assert('a'[0] == 'a');