let rt = import('std/runtime');

for i in range(0, 10) {
	if i == 1 { continue; }
	if i == 5 { break; }

	for j in range(0, 10) {
		if j == 1 { continue; }
		if j == 5 { break; }
		assert(rt.var_exists('i'));
		assert(rt.var_exists('j'));
	}
	assert(rt.var_exists('i'));
	assert(!rt.var_exists('j'));
}
assert(!rt.var_exists('i'));
assert(!rt.var_exists('j'));