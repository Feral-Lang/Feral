let sys = import('std/sys');

for i in range(0, 10) {
	if i == 1 { continue; }
	if i == 5 { break; }

	for j in range(0, 10) {
		if j == 1 { continue; }
		if j == 5 { break; }
		assert(sys.var_exists('i'));
		assert(sys.var_exists('j'));
	}
	assert(sys.var_exists('i'));
	assert(!sys.var_exists('j'));
}
assert(!sys.var_exists('i'));
assert(!sys.var_exists('j'));
