let lang = import( 'std/lang' );
let io = import( 'std/io' );

let str_t = lang.struct();
let __str__ in str_t = fn() {
	return 'object <' + lang.typeid( self ) + '> at ' + lang.addr( self );
};

let data_t = lang.struct( m_s = '', m_n = 0, s = fn() { return self.m_s; }, n = fn() { return self.m_n; } );

data_t.inherit( str_t );

let __str__ in data_t = fn() {
	return self.m_s + ': ' + str( self.m_n );
};

let d = data_t(), d1 = data( 'hi' ), d2 = data( 'hi', 10 );

for let i = 0; i < 10; {
	io.outln( 'hi there!' );
}
