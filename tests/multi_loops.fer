let sys = import('std/sys');

for let i = 0; i < 10; ++i {
	if i == 1 { continue; }
	if i == 5 { break; }

	for let j = 0; j < 10; ++j {
		if j == 1 { continue; }
		if j == 5 { break; }
		assert(sys.var_exists('i'));
		assert(sys.var_exists('j'));
	}
	assert(sys.var_exists('i'));
	assert(!sys.var_exists('j'));
}
assert(!sys.var_exists('i'));
assert(!sys.var_exists('j'));
