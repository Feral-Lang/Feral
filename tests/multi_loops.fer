let rt = import('std/runtime');

for let i = 0; i < 10; ++i {
	if i == 1 { continue; }
	if i == 5 { break; }

	for let j = 0; j < 10; ++j {
		if j == 1 { continue; }
		if j == 5 { break; }
		assert(rt.var_exists('i'));
		assert(rt.var_exists('j'));
	}
	assert(rt.var_exists('i'));
	assert(!rt.var_exists('j'));
}
assert(!rt.var_exists('i'));
assert(!rt.var_exists('j'));