#let io = import('std/io');
let vec = import('std/vec');

let list = vec.new(1, 3, 4, 5, 2);

let mid = list.len() / 2;

for let i = 0; i < mid; ++i {
	let tmp = list[ i ];
	list[ i ] = list[ list.len() - i - 1 ];
	list[ list.len() - i - 1 ] = tmp;
}

#io.println('reversed list is ', list);
assert(list == vec.new(2, 5, 4, 3, 1));