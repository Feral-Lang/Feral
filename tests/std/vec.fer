let vec = import('std/vec');

let v = vec.new(3, 2, 1, 4);
let v2 = vec.new();

assert(v.len() == 4);
assert(!v.empty());
assert(v.front() == 3);
assert(v.back() == 4);
assert(v.push(5).len() == 5);
assert(v.pop().len() == 4);
assert(v.insert(1, 5) == vec.new(3, 5, 2, 1, 4));
assert(v.erase(1) == vec.new(3, 2, 1, 4));
assert(v.lastidx() == 3);
assert(v.set(2, 5) == vec.new(3, 2, 5, 4));
assert(v.at(2) == 5);
assert(v[2] == 5);

for e in v.each() {
	v2.push(e);
}
assert(v == v2);