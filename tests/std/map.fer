let map = import('std/map');

let m = map.new(3, 'three', 1, 'one');

assert(m.len() == 2);
assert(!m.empty());
assert(m.insert(5, 'five') == map.new(5, 'five', 3, 'three', 1, 'one'));
assert(m.erase(5) == map.new(3, 'three', 1, 'one'));
assert(m.at(3) == 'three');
assert(m[1] == 'one');
assert(m[2] == nil);
assert(m.find(1) == true);

let m2 = map.new();
for e in m.each() {
	m2.insert(e.0, e.1);
}
assert(m == m2);