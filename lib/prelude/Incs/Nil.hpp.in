Var *nilEq(VirtualMachine &vm, ModuleLoc loc, Span<Var *> args,
           const StringMap<AssnArgData> &assnArgs)
{
    return args[1]->is<VarNil>() ? vm.getTrue() : vm.getFalse();
}

Var *nilNe(VirtualMachine &vm, ModuleLoc loc, Span<Var *> args,
           const StringMap<AssnArgData> &assnArgs)
{
    return !args[1]->is<VarNil>() ? vm.getTrue() : vm.getFalse();
}