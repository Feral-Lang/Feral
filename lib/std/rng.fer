loadlib('std/RNG');

let time = import('std/time');

seed(time.now().int());

"
  fn(from, to) -> Int
Returns a random number between [`from`, `to`].
"
let get = fn(from, to) {
    if from > to { raise('LHS should be less than or equal to RHS for random number generation'); }
    let res = getNative(to - from); # [0, to - from]
    return res + from;
};